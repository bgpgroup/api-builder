{
    "name": "bgpgroup/api-builder",
    "description": "Api Builder",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "BGP Group",
            "email": "bgp.developer@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "BgpGroup\\ApiBuilder\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BgpGroup\\ApiBuilder\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "BgpGroup\\ApiBuilder\\Providers\\"
            ]
        }
    }
}
